{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": 1,
    "no-empty": 1,
    "import/extensions": 0,
    "no-case-declarations": 1,
    "no-control-regex": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,
    "prefer-const": 1,
    "eqeqeq": 1,
    "array-callback-return": 1,
    "no-plusplus": 0,
    "consistent-return": 0,
    "no-await-in-loop": 1,
    "radix": 0,
    "no-unreachable": 1,
    "prefer-destructuring": 1,
    "prefer-promise-reject-errors": 1,
    "no-bitwise": 1,
    "no-loop-func": 1,
    "no-param-reassign": 1,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "no-prototype-builtins": 0,
    "class-methods-use-this": 0,
    "no-throw-literal": 1,
    "no-async-promise-executor": 1,
    "no-return-await": 1,
    "no-useless-escape": 1,
    "no-continue": 1,
    "import/no-dynamic-require": 0,
    "global-require": 1,
    "no-useless-constructor": 1
  },
  "globals": {
    "log": true,
    "include": true,
    "queue": true,
    "log2": true,
    "httpContext": true,
    "__dirname": true,
    "process": true,
    "setImmediate": true,
    "Buffer": true,
    "queueService": true
  }
}
